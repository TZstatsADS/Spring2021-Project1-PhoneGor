is_prime <- function(num) {
if (num == 1) {
return(FALSE)
} else if (num ==2 ) {
return(TRUE)
} else {
for (i in 2:(num-1)) {
if ((num %% i) == 0) {
return(FALSE)
}
}
}
return(TRUE)
}
