invgauss_mom_obj_factory <- function(x) {
moments = c(mean(x), var(x))
function(par) {
diff <- lnorm_moments(par) - moments
return(sum(diff^2))
}
}
invgauss_mom_obj_factory <- function(x) {
moments = c(mean(x), var(x))
function(par) {
diff <- invgauss_moments(par) - moments
return(sum(diff^2))
}
}
